<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="MAIN" Id="{c742ea4c-2437-4607-8d54-e505672bb457}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
    toggleOutput : BOOL := FALSE; // Output toggles every 5 seconds
    timer        : TON;           // Timer function block
    timerPeriod  : TIME := T#5s;  // Duration for high and low states
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
// Timer logic
timer.IN := TRUE;            // Timer always runs
timer.PT := timerPeriod;     // Set the timer duration

// Toggle logic
IF timer.Q THEN
    toggleOutput := NOT toggleOutput; // Toggle the output
    timer(IN := FALSE);               // Reset the timer
    timer(IN := TRUE);                // Restart the timer
END_IF

// Call the timer block
timer();
DIDO.op1 := toggleOutput;]]></ST>
    </Implementation>
